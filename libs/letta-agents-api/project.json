{
  "name": "letta-agents-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/letta-agents-api/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/letta-agents-api",
        "command": "(cd ../../apps/core/letta/server && bash generate_openapi_schema.sh) && openapi-rq -i ../../apps/core/letta/server/openapi_letta.json -c axios -o ./src/lib/_autogenerated"
      }
    },
    "monkeypatch": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/letta-agents-api",
        "command": "ts-node ./scripts/update-autogenerated-code.ts"
      }
    }
  }
}
