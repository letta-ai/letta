{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "types": ["jest"],
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["ES2023.Array", "es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "strictNullChecks": true,
    "paths": {
      "$web/*": ["apps/web/src/lib/*"],
      "@@letta-cloud/service-crm": ["libs/service-crm/src/index.ts"],
      "@letta-cloud/config-agent-starter-kits": [
        "libs/config-agent-starter-kits/src/index.ts"
      ],
      "@letta-cloud/config-environment-variables": [
        "libs/config-environment-variables/src/index.ts"
      ],
      "@letta-cloud/config-runtime": ["libs/config-runtime/src/index.ts"],
      "@letta-cloud/configure": ["scripts/configure/src/index.ts"],
      "@letta-cloud/desktop-builders": [
        "scripts/desktop-builders/src/index.ts"
      ],
      "@letta-cloud/lettuce-client": ["libs/sdk-lettuce/src/index.ts"],
      "@letta-cloud/lettuce-server": [
        "libs/sdk-lettuce/src/lib/lettuce-server.ts"
      ],
      "@letta-cloud/sdk-cloud-api": ["libs/sdk-cloud-api/src/index.ts"],
      "@letta-cloud/sdk-core": ["libs/sdk-core/src/index.ts"],
      "@letta-cloud/sdk-core-testing": ["libs/sdk-core-testing/src/index.ts"],
      "@letta-cloud/sdk-web": ["libs/sdk-web/src/index.ts"],
      "@letta-cloud/service-analytics": [
        "libs/service-analytics/src/events.ts"
      ],
      "@letta-cloud/service-analytics/client": [
        "libs/service-analytics/src/client/index.tsx"
      ],
      "@letta-cloud/service-analytics/server": [
        "libs/service-analytics/src/server/index.ts"
      ],
      "@letta-cloud/service-auth": ["libs/service-auth/src/index.ts"],
      "@letta-cloud/service-composio": ["libs/service-composio/src/index.ts"],
      "@letta-cloud/service-database": ["libs/service-database/src/index.ts"],
      "@letta-cloud/service-database-testing": [
        "libs/service-database-testing/src/index.ts"
      ],
      "@letta-cloud/service-email": ["libs/service-email/src/index.ts"],
      "@letta-cloud/service-feature-flags": [
        "libs/service-feature-flags/src/index.ts"
      ],
      "@letta-cloud/service-payments": ["libs/service-payments/src/index.ts"],
      "@letta-cloud/service-rbac": ["libs/service-rbac/src/index.ts"],
      "@letta-cloud/service-redis": ["libs/service-redis/src/index.ts"],
      "@letta-cloud/translations": ["libs/translations/src/index.ts"],
      "@letta-cloud/types": ["libs/types/src/index.ts"],
      "@letta-cloud/ui-ade-components": ["libs/ui-ade-components/src/index.ts"],
      "@letta-cloud/ui-ade-components/translations": [
        "libs/ui-ade-components/src/translations/index.ts"
      ],
      "@letta-cloud/ui-component-library": [
        "libs/ui-component-library/src/index.tsx"
      ],
      "@letta-cloud/ui-styles": ["libs/ui-styles/src/index.ts"],
      "@letta-cloud/ui-styles/*": ["utils/src/*"],
      "@letta-cloud/utils-client": ["libs/utils-client/src/index.ts"],
      "@letta-cloud/utils-server": ["libs/utils-server/src/index.ts"],
      "@letta-cloud/utils-shared": ["libs/utils-shared/src/index.ts"],
      "@letta-cloud/utils-types": ["libs/utils-types/src/index.ts"],
      "tmp-cloud-api-router": ["libs/tmp-cloud-api-router/src/index.ts"]
    },
    "jsx": "react"
  },
  "ts-node": {
    "require": ["tsconfig-paths/register"],
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "exclude": ["node_modules", "tmp", "*.config.js", "*.config.ts"]
}
