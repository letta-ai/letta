    "desktop:deps": "electron-builder install-app-deps && electron-builder install-app-deps",
    "desktop:package-core": "nx package desktop-core",
    "desktop:dev": "concurrently \"nx serve desktop-electron\" \"nx serve desktop-ui\"",
    "docs:dev": "nx dev docs",
    "docs:publish-api": "nx generate-openapi docs",
    "e2e": "nx open-cypress web",
    "e2e:ci": "nx run-many --all --skip-nx-cache --parallel --targets=e2e",
    "emails:dev": "nx dev email",
    "format": "nx format:write",
    "lint": "oxlint --react-plugin --import-plugin --max-warnings=200",
    "lettuce:dev": "nx dev lettuce",
    "prepare": "husky",
    "precommit": "npx lint-staged --concurrent true --relative",
    "slack-bot-says": "node ./scripts/slack-bot-says",
    "start": "nx start web -p 8080",
    "start:e2e": "cd apps/web && next start -p 3000",
    "start:api-stability": "nx start web -p 3000",
    "stress": "nx run stress:test --skip-nx-cache",
    "test": "npx nx run-many -p web ui-component-library utils-server ui-ade-components utils-shared  --skip-nx-cache --parallel --targets=test",
    "type-check": "nx run-many --all --skip-nx-cache --parallel --targets=type-check",
    "type-check:affected": "nx affected --target=type-check --uncommitted --parallel=3",
    "undertaker:dev": "nx dev credit-undertaker",
    "undertaker:build": "esbuild apps/credit-undertaker/src/main.ts --bundle --platform=node --outfile=dist/apps/credit-undertaker/main.js",
