    "afd:build": "nx run build agentfile-directory",
    "web:dev": "nx dev web",
    "web:dev:slow": "nx dev:slow web",
    "web:lint": "oxlint --react-plugin --import-plugin apps/web libs/ui-* libs/utils-client --max-warnings=200",
    "web:generate-web-only-openapi-spec": "nx generate-openapi web",
    "web:database:check": "nx run service-database:check",
    "web:database:generate": "nx run service-database:generate",
    "web:database:migrate": "nx run service-database:migrate",
    "web:database:migrate:ci": "cd libs/service-database && drizzle-kit migrate",
    "web:database:studio": "nx run service-database:studio",
    "web:test": "npx nx run-many -p web ui-component-library ui-ade-components utils-shared  utils-server --exclude=core  --skip-nx-cache --parallel --targets=test"
