{
  "$schema": "https://json-schema.org/schema",
  "$id": "NewComponent",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the name of your component, make sure it is in PascalCase"
    },
    "category": {
      "type": "string",
      "description": "The category of the component",
      "enum": ["core", "reusable", "ade", "framing", "marketing", "onboarding"],
      "default": "core",
      "x-prompt": "What is the category of your component?"
    }
  },
  "required": ["name"]
}
