{
  "name": "desktop-builders",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "scripts/configure/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/scripts/configure",
        "main": "scripts/configure/src/index.ts",
        "tsConfig": "scripts/configure/tsconfig.lib.json",
        "assets": [
          "scripts/configure/*.md",
          {
            "input": "./scripts/configure/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./scripts/configure/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./scripts/configure",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./scripts/configure",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    }
  }
}
