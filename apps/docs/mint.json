{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Starter Kit",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "font": {
    "family": "Overused Grotesk",
    "format": "woff2",
    "url": "../../libs/core-style-config/fonts/variable/overused-grotesk.woff2"
