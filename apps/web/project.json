{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "command": "next dev --turbopack"
      }
    },
    "dev:slow": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "command": "next dev"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --incremental  -p apps/web/tsconfig.json"
      }
    },
    "type-check:fast": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --incremental --assumeChangesOnlyAffectDirectDependencies -p apps/web/tsconfig.json"
      }
    },
    "generate-openapi": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "command": "ts-node ./scripts/generate-openapi.ts"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "apps/web/jest.config.ts"
      }
    }
  }
}
