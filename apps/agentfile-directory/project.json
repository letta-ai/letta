{
  "name": "agentfile-directory",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/agentfile-directory",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project agentfile-directory --web",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/agentfile-directory",
        "command": "next dev --turbopack"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p apps/cloud-api/tsconfig.app.json"
      }
    },
    "compile": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/agentfile-directory",
        "command": "next build"
      }
    }
  }
}
