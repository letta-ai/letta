{
  "name": "letta",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/letta",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project letta --web",
  "targets": {
    "type-check": {
      "executor": "@webpro/nx-tsc:tsc",
      "options": {
        "tsConfig": ["tsconfig.json"]
      }
    },
    "generate-openapi": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/letta",
        "command": "ts-node ./scripts/generate-openapi.ts"
      }
    }
  }
}
